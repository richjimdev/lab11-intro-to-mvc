@model List<FirstMVCApp.Models.TimePerson>

<head>
    <link href="~/StyleSheet.css" rel="stylesheet" />
</head>
<body>
    <div class="content">
        <h1 class="main-title">TIME</h1>
        <p>RESULTS</p>

        <table>
            <tr>
                <th>
                    Year
                </th>
                <th>
                    Name
                </th>
                <th>
                    Honor
                </th>
                <th>
                    Country
                </th>
                <th>
                    Birth Year
                </th>
                <th>
                    Death Year
                </th>
                <th>
                    Title
                </th>
            </tr>

            @foreach (var person in Model)
            {
                <tr>
                    <td>
                        @person.Year
                    </td>
                    <td>
                        @person.Name
                    </td>
                    <td>
                        @person.Honor
                    </td>
                    <td>
                        @person.Country
                    </td>
                    <td>
                        @person.Birth_Year
                    </td>
                    <td>
                        @person.DeathYear
                    </td>
                    <td>
                        @person.Title
                    </td>
                </tr>
            }
        </table>

    </div>
</body>